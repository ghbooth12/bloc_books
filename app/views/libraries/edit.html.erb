<h1>Edit Library</h1>

<form action="/libraries/update">
  <input type="hidden" name="id" value="<%= library.id %>">
  <label for="name">Name</label>
  <input type="text" id="name" name="library[name]" value="<%= library.name %>">
  <label for="year">Year</label>
  <input type="text" id="year" name="library[year]" value="<%= library.year %>">
  <button type="submit">Update</button>
</form>
